{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Enabled": true,
    "ConnId": "MySqllite",
    "HitRate": 50, // 值越大，优先级越高
    "Connection": "LocalData/LocalData.db", //sqlite只写数据库名就行
    "DBType": 2
  },
  "Jwt": {
    "Issuer": "JCF.WebApi",
    "Audience": "JCF.WebApiClient",
    "SecretKey": "THIS_IS_A_VERY_LONG_SECRET_KEY_123456",
    "AccessTokenMinutes": 15
  }
}
